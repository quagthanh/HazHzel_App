{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js debug",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--inspect",
        "node_modules/next/dist/bin/next",
        "dev"
      ],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
